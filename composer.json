{
    "name": "vinelab/laravel-editor",
    "description": "Markdown Text Editor (WYSIWYG) with social network embeds support and a nifty json output.",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        },
        {
          "name": "Mahmoud Zalt",
          "email": "mahmoud@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "michelf/php-markdown": "1.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*"
    },
    "autoload": {
        "psr-0": {
            "Vinelab\\Editor\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\Editor\\EditorServiceProvider"
            ]
        }
    }
}
